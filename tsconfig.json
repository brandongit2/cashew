{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["esnext"],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "paths": {"@c/*": ["src/*"]}
  }
}
